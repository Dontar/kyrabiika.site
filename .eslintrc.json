{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": [ "warn", "double" ],
    "semi": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ]
}
